<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Constituent_or_Org_Must_Be_Present</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(ucinn_ascendv2__Contact__c), ISBLANK(ucinn_ascendv2__Account__c))</errorConditionFormula>
    <errorMessage>You must enter a constituent or an organization in order to create an involvement</errorMessage>
</ValidationRule>
