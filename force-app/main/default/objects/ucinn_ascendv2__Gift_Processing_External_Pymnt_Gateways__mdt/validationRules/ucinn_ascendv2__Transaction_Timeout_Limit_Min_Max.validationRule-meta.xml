<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ucinn_ascendv2__Transaction_Timeout_Limit_Min_Max</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ucinn_ascendv2__Transaction_Timeout_Limit__c &gt; 120,  ucinn_ascendv2__Transaction_Timeout_Limit__c &lt; 1)</errorConditionFormula>
    <errorDisplayField>ucinn_ascendv2__Transaction_Timeout_Limit__c</errorDisplayField>
    <errorMessage>Transaction Timeout Limit must be greater than or equal to 1 seconds and less than or equal to 120 seconds</errorMessage>
</ValidationRule>
