<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ucinn_ascendv2__Check_End_Date_is_After_Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
$Setup.ucinn_ascendv2__Bypass_Settings__c.ucinn_ascendv2__Bypass_Validation_Rules__c  &lt;&gt; TRUE,
ucinn_ascendv2__Start_Date__c &gt; ucinn_ascendv2__End_Date__c)</errorConditionFormula>
    <errorMessage>Invalid start and end dates. Start date should be less than the end date.</errorMessage>
</ValidationRule>
