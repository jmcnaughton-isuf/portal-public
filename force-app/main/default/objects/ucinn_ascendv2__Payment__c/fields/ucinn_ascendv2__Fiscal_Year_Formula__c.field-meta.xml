<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ucinn_ascendv2__Fiscal_Year_Formula__c</fullName>
    <deprecated>false</deprecated>
    <description>Calculates the Fiscal Year for the Giving History</description>
    <externalId>false</externalId>
    <formula>IF (MONTH(ucinn_ascendv2__Credit_Date__c ) &gt;= 7  &amp;&amp; MONTH(ucinn_ascendv2__Credit_Date__c ) &lt;= 12 ,YEAR(ucinn_ascendv2__Credit_Date__c )+1 , YEAR( ucinn_ascendv2__Credit_Date__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Fiscal Year</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
