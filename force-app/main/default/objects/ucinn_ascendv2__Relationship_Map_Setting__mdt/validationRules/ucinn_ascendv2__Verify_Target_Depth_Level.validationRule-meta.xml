<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ucinn_ascendv2__Verify_Target_Depth_Level</fullName>
    <active>true</active>
    <description>Maximum Depth Level must be 1-9 (inclusive).</description>
    <errorConditionFormula>OR( ucinn_ascendv2__Target_Depth_Level__c &lt; 1, ucinn_ascendv2__Target_Depth_Level__c &gt; 9)</errorConditionFormula>
    <errorDisplayField>ucinn_ascendv2__Target_Depth_Level__c</errorDisplayField>
    <errorMessage>Maximum Depth Level must be 1-9 (inclusive).</errorMessage>
</ValidationRule>
