<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Timeline_Date__c</fullName>
    <description>Formula fields to determine what date to use for the Timeline function on the contact\account lightning pages. Based on in row data, not pulling from other objects.</description>
    <externalId>false</externalId>
    <formula>IF(  ISPICKVAL(ucinn_ascendv2__Assignment_Type__c, &apos;Qualification Manager&apos;)  ,  Qualify_Disqualify_Date__c ,  ucinn_ascendv2__Assignment_Start_Date__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Timeline Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
