<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Terms_and_Conditions</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Has_Terms_and_Conditions__c == true, LEN(Terms_and_Conditions_Text__c) == 0)</errorConditionFormula>
    <errorDisplayField>Terms_and_Conditions_Text__c</errorDisplayField>
    <errorMessage>Terms and Conditions Text must be filled out.</errorMessage>
</ValidationRule>
